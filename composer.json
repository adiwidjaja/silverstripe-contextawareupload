{
    "name": "symbiote/silverstripe-YOUR_MODULE_NAME",
    "description": "YOUR DESCRIPTION HERE",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jake Bentvelzen",
            "email": "jake@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\YOUR_MODULE_NAME\\": "src/",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\YOUR_MODULE_NAME\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
